@model CarRentalsSystem.Models.Car

<ul>
    <li>@Model.Id</li>
    <li>@Model.Make</li>
    <li>@Model.Name</li>
    <li>@Model.Year</li>   
    <li>@Model.PlateNo </li>    
    <li>@Model.Features</li>
    <li>@Model.Price</li>
    <li>@Model.BreakdownPrice</li>
